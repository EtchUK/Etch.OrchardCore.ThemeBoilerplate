{
    "author": "Moov2 Ltd.",
    "classifications": [
        "OrchardCore",
        "Web"
    ],
    "name": "Orchard Core Theme Boilerplate",
    "identity": "Moov2.OrchardCore.ThemeBoilerplat",
    "shortName": "orchardcore-themeboilerplate",
    "tags": {
        "language": "C#"
    },
    "sourceName": "Moov2.OrchardCore.ThemeBoilerplate",
    "preferNameDirectory": "true",
    "symbols": {
        "author": {
            "type": "parameter",
            "defaultValue": "Unknown",
            "replaces": "Moov2 Ltd."
        },
        "description": {
            "type": "parameter",
            "defaultValue": "Theme for Orchard Core.",
            "replaces": "Theme boilerplate is our starting point for building Orchard Core themes."
        },
        "includeReadme": {
            "type": "parameter",
            "dataType": "bool",
            "defaultValue": "true"
        },
        "themeName": {
            "type": "parameter",
            "defaultValue": "Orchard Core Theme",
            "replaces": "Boilerplate Theme"
        },
        "website": {
            "type": "parameter",
            "defaultValue": "https://orchardcore.readthedocs.io/en/latest/",
            "replaces": "https://moov2.com"
        }
    },
    "sources": [
        {
            "modifiers": [
                {
                    "condition": "(!includeReadme)",
                    "exclude": [
                        "README.md"
                    ]
                }
            ]
        }
    ]
}