{
    "author": "Etch UK",
    "classifications": [
        "OrchardCore",
        "Web"
    ],
    "name": "Orchard Core Theme Boilerplate",
    "identity": "Etch.OrchardCore.ThemeBoilerplate",
    "shortName": "orchardcore-themeboilerplate",
    "tags": {
        "language": "C#"
    },
    "sourceName": "Etch.OrchardCore.ThemeBoilerplate",
    "preferNameDirectory": "true",
    "symbols": {
        "author": {
            "type": "parameter",
            "defaultValue": "Unknown",
            "replaces": "Etch UK"
        },
        "description": {
            "type": "parameter",
            "defaultValue": "Theme for Orchard Core.",
            "replaces": "Theme boilerplate is our starting point for building Orchard Core themes."
        },
        "themeName": {
            "type": "parameter",
            "defaultValue": "Orchard Core Theme",
            "replaces": "Boilerplate Theme"
        },
        "version": {
            "type": "parameter",
            "defaultValue": "0.0.1-rc1",
            "replaces": "0.6.6-rc2"
        },
        "website": {
            "type": "parameter",
            "defaultValue": "https://orchardcore.readthedocs.io/en/latest/",
            "replaces": "https://etchuk.com"
        }
    }
}