<!DOCTYPE html>
<html class="no-js" lang="{{ Culture.Name }}">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {% render_section "HeadMeta", required: false %}
        <title>{% page_title "", position: "after", separator: "" %}</title>
        {% resources type: "Meta" %}
        
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/manuale-v8-latin-regular.woff" as="font" type="font/woff" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/manuale-v8-latin-regular.woff2" as="font" type="font/woff2" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/manuale-v8-latin-700.woff" as="font" type="font/woff" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/manuale-v8-latin-700.woff2" as="font" type="font/woff2" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/work-sans-v9-latin-regular.woff" as="font" type="font/woff" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/work-sans-v9-latin-regular.woff2" as="font" type="font/woff2" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/work-sans-v9-latin-700.woff" as="font" type="font/woff" crossorigin="anonymous">
        <link rel="preload" href="/Etch.OrchardCore.ThemeBoilerplate/work-sans-v9-latin-700.woff2" as="font" type="font/woff2" crossorigin="anonymous">

        {% if Environment.IsDevelopment %}
            <script type="module" defer src="https://localhost:5173/@vite/client"></script>
            <script type="module" defer src="https://localhost:5173/Assets/Scripts/index.ts"></script>
            <link rel="stylesheet" href="https://localhost:5173/Assets/Styles/index.scss">
        {% else %}
            {% style src:"/Etch.OrchardCore.ThemeBoilerplate/styles.css", append_version:"true" %}
		    {% script src:"/Etch.OrchardCore.ThemeBoilerplate/scripts.js", append_version:"true", async: "true" %}
        {% endif %}
        
        {% resources type: "HeadLink" %}
        {% resources type: "Stylesheet" %}
        {% resources type: "HeadScript" %}
    </head>
    <body class="animate">
        {% render_section "Header", required: false %}

        <main>
            {% render_section "BeforeContent" %}
            {% render_section "Content" %}
            {% render_section "AfterContent" %}
        </main>

        {% render_section "Footer", required: false %}

        {% resources type: "FootScript" %}
    </body>
</html>
